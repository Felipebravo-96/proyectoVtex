{
    "flex-layout.row#newsletter":{
        "title": "newsletter section",
        "children": [
            "flex-layout.col#newsletter",
            "flex-layout.col#facebook"
        ],
        "props":{
            "verticalAlign": "middle",
            "horizontalAlign": "center"
        }   
    },
    "flex-layout.col#newsletter":{
        "title": "newsletter",
        "children":[
            "rich-text#titulo__newsletter",
            "rich-text#text__newsletter",
            "newsletter-form"
           
           
        ],
        "props":{
            "verticalAlign": "middle",
            "horizontalAlign": "center",
            "blockClass":"global__newsletter--container"
        }
    },
    //form
    "newsletter-form": {
        "title": "Newsletter Form",
        "children":[
            "newsletter-input-name",
            "newsletter-input-email",
            "newsletter-submit"
        ],
        "props":{
            "blockClass":"global__newsletter"
            
        }
    
    },
    "newsletter-input-name":{
        "title": "Name Form",
        "props":{
            "placeholderText": "Nombre",
            "errorMessage": "Introduce tu nombre"
        }
    },
    "newsletter-input-email":{
        "title": "Email Form",
        "props":{
            "placeholderText": "Tu E-mail",
            "errorMessage": "Introduce una  direccion E-mail correcta"
        }
    },
    "newsletter-submit":{
        "props":{
            "submitButtonLabel": "INSCRIBIRSE"

        }
    },



    "rich-text#titulo__newsletter":{
        "title": "newsletter title",
        "props": {
            "text": "NUESTRO NEWSLETTER",
            "blockClass": "newsletter_title"
        }
    },
    "rich-text#text__newsletter":{
        "title": "suscribe",
        "props": {
            "text": "¡Suscribete para recibir nuestras últimas novedades!"
        }
    },
    "link#suscribe":{
        "title": "suscribe button",
        "props": {
            "label": "INSCRIBIRSE",
            "href": "/",
            "displayMode": "button"
        }
    },
    //facebook
    "flex-layout.col#facebook":{
        "title": "facebook",
        "children":[
            "rich-text#titulo__facebook",
            "flex-layout.col#facebook__mainSection"
        ],
        "props":{
            "verticalAlign": "middle",
            "horizontalAlign": "center",
            "blockClass": "main__container--facebook"
        }
    },
    "rich-text#titulo__facebook":{
        "title": "facebook title",
        "props": {
            "text": "ESTAMOS EN FACEBOOK",
            "blockClass": "facebook_title"
        }
    },
    "flex-layout.col#facebook__mainSection":{
        "title": "facebook section",
        "children":[
            "flex-layout.row#facebook__section--one",
            "flex-layout.col#facebook__section--two",
            "flex-layout.col#facebook__section--three"

        ],
        "props":{
            "blockClass": "container__facebook"
        }
    },
    "flex-layout.row#facebook__section--one":{
        "title": "facebook section 1",
        "children":[
            "logo#facebook--logo",
            "rich-text#text__facebook"
        ],
        "props":{
            "colSizing": "equal",
            "colJustify": "around"
        }
    },
    "logo#facebook--logo":{
        "title": "Logo  facebook",
        "props":{
          "url" : "assets/img/home.png",
          "width": 65 
        }
      },
      "rich-text#text__facebook":{
        "title": "text Rooster",
        "props": {
            "text": "Tenis Rooster al Horno | ZAPATOS 100% COLOMBIANOS",
            "blockClass": "text__facebook"
        }
      },
      "flex-layout.col#facebook__section--two":{
        "children":[
            "link#facebook__section--one"
        ],
        "props":{
            "blockClass":"container__like_button"
        }
      },
      "flex-layout.col#facebook__section--three":{
        "children":[
            "link#facebook__section--two" 
        ],
        "props":{
            "blockClass":"container__facebook_link"
        }
      },
      "link#facebook__section--one":{
        "title": "like button",
        "props": {
            "label": "Me Gusta",
            "href": "/",
            "displayMode": "button",
            "blockClass": "like_button"
        }
      },
      "link#facebook__section--two":{
        "title": "facebook button",
        "props": {
            "label": "Visitá nuestra página",
            "href": "/",
            "blockClass": "facebook_link"
        }
      }
}